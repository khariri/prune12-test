<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<record id="nofas_masuk_tree" model="ir.ui.view">
		<field name="name">Laporan Pemasukan Tree</field>
		<field name="model">djbc.nofas_masuk</field>
		<field name="arch" type="xml">
			<tree create="0" edit="0">
				<field name="jenis_dok"/>
				<field name="no_aju" invisible="1"/>
				<field name="tgl_aju" invisible="1"/>
				<field name="no_dok"/>
				<field name="tgl_dok"/>
				<field name="no_penerimaan"/>
				<field name="tgl_penerimaan"/>
				<field name="no_bl" invisible= "1"/>
				<field name="tgl_bl" invisible="1"/>
				<field name="no_cont" invisible= "1"/>
				<field name="pengirim"/>
				<field name="pemilik" invisible= "1"/>
				<field name="hs_code" invisible= "1" />
				<field name="kode_barang"/>
				<field name="nama_barang"/>
				<field name="lot_id" invisible="1" />
				<field name="jumlah"/>
				<field name="satuan"/>
				<field name="jumlah_kemasan" invisible="1"/>
				<field name="satuan_kemasan" invisible="1"/>
				<field name="nilai"/>
				<field name="currency"/>
				<field name="location" invisible="1"/>
				<field name="warehouse"/>
				<field name="alm_wh" invisible="1"/>
				<field name="kota_wh" invisible="1"/>
				
			</tree>
		</field>
	</record> 



	<record id="nofas_masuk_form" model="ir.ui.view">
		<field name="name">Laporan Pemasukan Form</field>
		<field name="model">djbc.nofas_masuk</field>
		<field name="arch" type="xml">
			<form create="0" edit="0">
				<group>
					<group>
						<field name="jenis_dok"/>
						<field name="no_aju"/>
						<field name="tgl_aju"/>
						<field name="no_dok"/>
						<field name="tgl_dok"/>
						<field name="no_penerimaan"/>
						<field name="tgl_penerimaan"/>
						<field name="no_bl"/>
						<field name="tgl_bl"/>
						<field name="no_cont"/>
						<field name="pengirim"/>
						<field name="pemilik"/>
						<field name="hs_code"/>
						
					</group>
					<group>
						<field name="kode_barang"/>
						<field name="nama_barang"/>
						<field name="lot_id"/>
						<field name="jumlah"/>
						<field name="satuan"/>
						<field name="jumlah_kemasan"/>
						<field name="satuan_kemasan"/>
						<field name="nilai"/>
						<field name="currency"/>
						<field name="location"/>
						<field name="warehouse"/>
						<field name="alm_wh"/>
						<field name="kota_wh"/>
					</group>
				</group>
			</form>
		</field>
	</record>

	<record id="nofas_masuk_action" model="ir.actions.act_window">
		<field name="name">Laporan Pemasukan</field>
		<field name="type">ir.actions.act_window</field>
		<field name="res_model">djbc.nofas_masuk</field>
		<field name="view_type">form</field>
		<field name="view_mode">tree,form</field>
	</record>


</odoo>
